{
  "name": "svelte-boilerplate",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently 'svelte-kit dev' 'tailwindcss -i ./src/css/style.css -o ./src/style.css -c ./tailwind.config.cjs --postcss -w'",
    "prebuild": "NODE_ENV=production tailwindcss -i ./src/css/style.css -o ./src/style.css -c ./tailwind.config.cjs --postcss",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "felte": "0.9.0",
    "praecox-datepicker": "1.1.7",
    "sswr": "1.3.1",
    "svelte-feather-icons": "3.5.0",
    "svelte-seo": "1.3.1",
    "svelte-yup": "^1.0.5",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.0",
    "@tailwindcss/typography": "0.5.0",
    "@types/node": "17.0.5",
    "autoprefixer": "10.4.0",
    "concurrently": "^6.5.1",
    "cssnano": "5.0.14",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "postcss": "8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "3.1.0",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "svelte": "3.44.3",
    "svelte-preprocess": "4.10.1",
    "swiper": "7.4.1",
    "tailwindcss": "3.0.7",
    "vite": "2.6.14"
  },
  "type": "module"
}
