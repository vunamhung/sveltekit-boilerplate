  "scripts": {
    "dev": "TAILWIND_MODE=watch svelte-kit dev",
    "prebuild": "NODE_ENV=production tailwind -i ./src/css/style.css -o ./src/style.css -c ./tailwind.config.cjs --postcss",
    "build": "svelte-kit build",
