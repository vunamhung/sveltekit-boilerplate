    "@tailwindcss/typography": "0.4.1",
    "@types/node": "16.10.9",
    "autoprefixer": "10.3.7",
    "cssnano": "5.0.8",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "postcss": "8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "3.1.0",
    "prettier": "2.4.1",
