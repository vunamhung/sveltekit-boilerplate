    "@tailwindcss/line-clamp": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@types/node": "16.9.2",
    "autoprefixer": "10.3.4",
    "cssnano": "5.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "postcss": "8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "3.1.0",
    "prettier": "2.4.1",
    "prettier-plugin-svelte": "2.4.0",
    "svelte": "3.42.6",
