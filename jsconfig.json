{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
      "$lib/*": ["src/lib/*"],
      "$components/*": ["src/components/*"],
      "$stores/*": ["src/stores/*"]
    }
  },
  "include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
